<!-- content heading -->
<% if (title) { %>
    <h1><%= title %></h1>
    <% } else { res.redirect('/') } %> 
    <!-- Message Block -->
    <% if (messages && messages.notice) { %>
    <p class="flash-message"><%= messages.notice %></p>
    <% } %> <% if (errors) { %>
    <ul class="notice">
      <% errors.array().forEach(error => { %>
      <li><%= error.msg %></li>
      <% }) %>
    </ul>
    <% } %>

<!-- login form -->
<div> 
  <a href="/account/manage">Manage</a>
</div>
